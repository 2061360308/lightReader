/**app.wxss**/
page {
    background: #f6f6f6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;

    --dark-background: #343434;
    --dark-text-primary: #b2b2b2;

    --white-background: #f6f6f6;
    --white-text-primary: #343434;
}


.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}

button {
    background: initial;
}

button:focus {
    outline: 0;
}

button::after {
    border: none;
}


/* iconfont */
@import '/static/iconfont/iconfont.wxss';

[class^="icon"],
[class^=" icon"] {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
/* end */